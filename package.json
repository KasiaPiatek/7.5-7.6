{
  "name": "nauka-gita-5",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.18.13",
    "glob-run": "^0.1.6",
    "jshint": "^2.9.5",
    "nu-html-checker": "^0.1.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "parallelshell": "^3.0.2"
  },
  "scripts": {
    "test": "npm run test:html",
    "test:html": "glob-run nu-html-checker *.html",
    "watch": "browser-sync start --server --files libs/*.css *.html",
    "watch:browsersync": "npm run watch",
    "watch:sass": "sass --watch sass:css",
    "watch:autoprefixer": "onchange -d 100 libs/style.css -- autoprefixer-cli -o libs/style.prefixed.css libs/style.css",
    "build:clean": "rimraf libs/*.css",
    "build": "npm run build:clean && npm run build:sass && npm run build:autoprefixer && npm run test",
    "build:sass": "sass --sourcemap=none --update libs:css",
    "build:autoprefixer": "cd libs && autoprefixer-cli -o libs/style.prefixed.css libs/style.css && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KasiaPiatek/7.5.git"
  },
  "keywords": [],
  "author": "",
